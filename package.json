{
  "name": "drama-keeper",
  "version": "1.0.0",
  "description": "Keep a record of the films watched",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --files server.ts --trace-warnings",
    "test": "jest",
    "eslint-staged": "LIST=`git diff --cached --name-only | grep .*\\.ts | grep -v json`; if [ \"$LIST\" ]; then eslint $LIST && prettier $LIST --write; fi",
    "dev": "concurrently \"yarn start\" \"cd client/ && yarn start\""
  },
  "pre-commit": [
    "eslint-staged"
  ],
  "repository": "https://github.com/Ruberoni/drama-keeper",
  "author": "Ruben Paredes",
  "license": "MIT",
  "private": true,
